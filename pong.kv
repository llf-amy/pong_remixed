#:kivy 1.9.0
#:include debug.kv
PongGame:

<PongGame>:
    gameworld: _gameworld
    DebugLabel:
        text: 'PongGame'
    GameWorld:
        id: _gameworld
        gamescreenmanager: _gamescreenmanager
        size_of_gameworld: 100*1024
        size_of_entity_block: 128
        system_count: 2
        zones: {'general':10000}
        DebugLabel:
            text:'GameWorld'
    GameScreenManager:
        id: _gamescreenmanager
        gameworld: _gameworld
        size: root.size
        pos: root.pos
        GameScreen:
            name: 'main'
